# Basic binds

super + r
	urxvt -e ranger
super + c
	urxvt -e calcurse
super + g
	urxvt -e gotop -p
super + y
	urxvt -e octave -q
super + b
	urxvt -e alsamixer
super + d 
	Menu
super + e
	brave	
super + shift + p
	keepass ~/Sync/Password_Database.kdbx 
super + p 
	rofi -show run -theme Pop-Dark	-font "Iosevka 14"
super + shift + s
	loginctl suspend
super + shift + Return
	urxvt -e nvim 
#	rofi -show ssh -theme Pop-Dark	-font "Iosevka 14"
super + shift + n 
	urxvt -e newsboat
