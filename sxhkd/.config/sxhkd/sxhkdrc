# Basic binds

# description: Terminal File Manager
super + r
	st   ranger
# description: Terminal Resources Manager
super + g
	st   gotop -p
# description: Octave
super + y
	st   octave -q
#description: Alsamixer
super + b
	st  alsamixer
#description: Antonio Menu
super + d 
	/home/tony/bin/Menu
#description: Firefox
super + e
	firefox
#description: Password Manager
super + shift + p
	keepass ~/Sync/Password_Database.kdbx 
#description: Rofi drun
super + p 
	rofi -show run -theme Pop-Dark	-font "Iosevka 14"
#description: Suspend
super + shift + s
	loginctl suspend
#description: Emacs Client
super + shift + Return
	emacsclient -nc
#description: Help With keys
super + shift + h
	help-bindings
